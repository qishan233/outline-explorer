{
  "name": "outline-explorer",
  "displayName": "Outline Explorer",
  "description": "This extension supports displaying outlines in a manner similar to the explorer, inspired by the \"Show Member\" feature in JetBrains IDEs.",
  "version": "0.0.6",
  "icon": "resources/extension-icon.png",
  "publisher": "qishan233",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "vscode": "^1.95.0"
  },
  "keywords": [
    "Outline",
    "Explorer",
    "JetBrains"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsWelcome": [
      {
        "view": "outline-explorer",
        "contents": "%view.welcome.content%"
      }
    ],
    "commands": [
      {
        "command": "outline-explorer.item-clicked",
        "title": "%command.title.click%"
      },
      {
        "command": "outline-explorer.refresh",
        "title": "%command.title.refresh%",
        "icon": "$(refresh)"
      },
      {
        "command": "outline-explorer.expand-all",
        "title": "%command.title.expand-all%",
        "icon": "$(expand-all)"
      },
      {
        "command": "outline-explorer.collapse-all",
        "title": "%command.title.collapse-all%",
        "icon": "$(collapse-all)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "outline-explorer",
          "title": "Outline Explorer",
          "icon": "resources/view-icon.svg"
        }
      ]
    },
    "views": {
      "outline-explorer": [
        {
          "id": "outline-explorer",
          "name": "%extension.name%",
          "canToggleVisibility": true,
          "icon": "resources/view-icon.svg"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "outline-explorer.refresh",
          "when": "view == outline-explorer && (viewItem == file || viewItem == folder)",
          "group": "navigation"
        }
      ],
      "view/title": [
        {
          "command": "outline-explorer.expand-all",
          "when": "view == outline-explorer && code-lens.context.can-expand",
          "group": "navigation"
        },
        {
          "command": "outline-explorer.collapse-all",
          "when": "view == outline-explorer && code-lens.context.can-collapse",
          "group": "navigation"
        }
      ]
    }
  },
  "pricing": "Free",
  "repository": {
    "type": "git",
    "url": "https://github.com/qishan233/outline-explorer"
  },
  "categories": [
    "Other"
  ],
  "homepage": "https://github.com/qishan233/outline-explorer/blob/main/README.md",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "uuid": "^13.0.0"
  }
}