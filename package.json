{
  "name": "outline-explorer",
  "displayName": "Outline Explorer",
  "description": "This extension supports displaying outlines in a manner similar to the explorer, inspired by the \"Show Member\" feature in JetBrains IDEs.",
  "version": "0.1.0",
  "icon": "resources/extension-icon.png",
  "publisher": "qishan233",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "vscode": "^1.95.0"
  },
  "keywords": [
    "Outline",
    "Explorer",
    "JetBrains"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Outline Explorer",
      "properties": {
        "outline-explorer.unsupportedFileExtensions": {
          "type": "array",
          "default": [
            ".png",
            ".jpg",
            ".jpeg",
            ".gif",
            ".bmp",
            ".ico",
            ".svg",
            ".webp",
            ".tiff",
            ".tif",
            ".mp4",
            ".avi",
            ".mov",
            ".wmv",
            ".flv",
            ".mkv",
            ".webm",
            ".mp3",
            ".wav",
            ".flac",
            ".aac",
            ".ogg",
            ".wma",
            ".m4a",
            ".zip",
            ".rar",
            ".7z",
            ".tar",
            ".gz",
            ".bz2",
            ".xz",
            ".exe",
            ".dll",
            ".so",
            ".dylib",
            ".bin",
            ".pdf",
            ".doc",
            ".docx",
            ".xls",
            ".xlsx",
            ".ppt",
            ".pptx",
            ".db",
            ".sqlite",
            ".class",
            ".pyc",
            ".o",
            ".a",
            ".lib",
            ".vsix"
          ],
          "items": {
            "type": "string"
          },
          "markdownDescription": "%configuration.unsupportedFileExtensions.description%"
        }
      }
    },
    "viewsWelcome": [
      {
        "view": "outline-explorer",
        "contents": "%view.welcome.content%"
      }
    ],
    "commands": [
      {
        "command": "outline-explorer.item-clicked",
        "title": "%command.title.click%"
      },
      {
        "command": "outline-explorer.refresh",
        "title": "%command.title.refresh%",
        "icon": "$(refresh)"
      },
      {
        "command": "outline-explorer.expand-1-level",
        "title": "%command.title.expand-1-level%",
        "icon": "$(expand-all)"
      },
      {
        "command": "outline-explorer.expand-2-levels",
        "title": "%command.title.expand-2-levels%",
        "icon": "$(expand-all)"
      },
      {
        "command": "outline-explorer.collapse-all",
        "title": "%command.title.collapse-all%",
        "icon": "$(collapse-all)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "outline-explorer",
          "title": "Outline Explorer",
          "icon": "resources/view-icon.svg"
        }
      ]
    },
    "views": {
      "outline-explorer": [
        {
          "id": "outline-explorer",
          "name": "%extension.name%",
          "canToggleVisibility": true,
          "icon": "resources/view-icon.svg"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "outline-explorer.expand-2-levels",
          "when": "view == outline-explorer && (viewItem !== unsupported)",
          "group": "navigation@1"
        },
        {
          "command": "outline-explorer.collapse-all",
          "when": "view == outline-explorer && (viewItem !== unsupported)",
          "group": "navigation@2"
        },
        {
          "command": "outline-explorer.refresh",
          "when": "view == outline-explorer && (viewItem == file || viewItem == folder)",
          "group": "navigation@3"
        }
      ],
      "view/title": [
        {
          "command": "outline-explorer.expand-1-level",
          "when": "view == outline-explorer && code-lens.context.can-expand",
          "group": "navigation"
        },
        {
          "command": "outline-explorer.collapse-all",
          "when": "view == outline-explorer && code-lens.context.can-collapse",
          "group": "navigation"
        }
      ]
    }
  },
  "pricing": "Free",
  "repository": {
    "type": "git",
    "url": "https://github.com/qishan233/outline-explorer"
  },
  "categories": [
    "Other"
  ],
  "homepage": "https://github.com/qishan233/outline-explorer/blob/main/README.md",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "js-yaml": "^4.1.1",
    "uuid": "^13.0.0",
    "vscode-nls": "^5.2.0"
  }
}